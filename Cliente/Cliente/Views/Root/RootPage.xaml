<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewm="clr-namespace:Cliente.Views;assembly=Cliente"
    x:Class="Cliente.Views.RootPage"
    Title="Root">
    <MasterDetailPage.Master>
        <NavigationPage
            Icon="slideout.png"
            Title="Menu">
            <x:Arguments>
                <viewm:RootMenu
                    x:Name="MasterMenu" />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Master>
</MasterDetailPage>